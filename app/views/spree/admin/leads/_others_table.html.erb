					<!-- <td> <%# other.black_tie || "N/A" %> </td> -->
					<!-- <td> <%# other.juniors || "N/A" %> </td> -->
					<!-- <td> <%# other.number_seniors || "N/A"%> </td> -->
					<!-- <td> <%# other.number_juniors  || "N/A" %> </td> -->
					<!-- <td> <%# other.first_name  || "N/A" %> </td> -->
					<!-- <td> <%# other.last_name  || "N/A" %> </td> -->
					<!-- <td> <%# other.contact_title || "N/A"  %> </td> -->
					<!-- <td> <%# other.contact_phone  || "N/A" %> </td> -->
					<%# user # Spree::User.find other.user_id %>
					<!-- <td> <%# user && user.email  || "N/A" %> </td> -->
					<!-- <td> <%# other.group_code && other.group_code.number || "N/A" %> </td> -->
					<!-- <td> <%# other.school_fundraiser %> </td> -->
					<!-- <td> <%# other.prom_rep %> </td> -->
    		</tr>
    	</tbody>
		<% end %>  
  </table>
</div>



